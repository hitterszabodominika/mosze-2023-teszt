# Klónozza a repository-t a saját gépére
git clone <repository_url>

# Navigáljon a repository mappájába
cd mosze-2023-teszt

# Másolja a main.cpp fájlt a repository mappájába
cp /path/to/main.cpp .

# Adjuk hozzá a fájlt a stage-hez
git add main.cpp

# Commitoljuk a változást
git commit -m "Initial commit: main.cpp hozzáadva"

# Push-oljuk a változást a távoli repository-ba
git push origin main
